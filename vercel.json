{
  "buildCommand": "npm run docs:build",
  "outputDirectory": "site/.vitepress/dist",
  "installCommand": "npm install",
  "devCommand": "",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
